<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>User Report</title>

    <!-- Custom styles for this template -->
    <link rel="stylesheet" href="/styles.css" />

  </head>
  <body>
    <table>
        <h1>Male</h1>
        <tr>
            <td>Nationality</td>
            <td> 0-30</td>
            <td> 30-50</td>
            <td> 50 above</td>
        </tr>
        <%  Users.male.forEach(value => {  %>
        <tr>
            <td><%= value.nat  %></td>
            <td><%= value.Range0To30  %></td>
            <td><%= value.Range30To50  %></td>
            <td><%= value.Range50AndAbove  %></td>
            <% })  %>
        </tr>
    </table>
    <table>
        <h1>Female</h1>
        <tr>
            <td>Nationality</td>
            <td> 0-30</td>
            <td> 30-50</td>
            <td> 50 above</td>
        </tr>
        <%  Users.female.forEach(value => {  %>
        <tr>
            <td><%= value.nat  %></td>
            <td><%= value.Range0To30  %></td>
            <td><%= value.Range30To50  %></td>
            <td><%= value.Range50AndAbove  %></td>
            <% })  %>
        </tr>
    </table>
  </body>
  </html>